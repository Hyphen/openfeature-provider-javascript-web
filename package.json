{
  "name": "@hyphen/openfeature-web-provider",
  "version": "0.1.0",
  "description": "The official Hyphen Toggle OpenFeature Web Provider",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "test": "vitest --coverage --run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hyphen/openfeature-provider-javascript-web.git"
  },
  "keywords": [
    "toggle",
    "openfeature",
    "provider",
    "hyphen",
    "web"
  ],
  "author": "@hyphen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hyphen/openfeature-provider-javascript-web/issues"
  },
  "homepage": "https://github.com/Hyphen/openfeature-provider-javascript-web#readme",

  "dependencies": {
    "@openfeature/web-sdk": "^1.3.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json*"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
